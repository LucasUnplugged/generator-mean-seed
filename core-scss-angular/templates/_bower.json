{
	"name": "<%= optAppName %>",
	"version": "0.0.0",
	"homepage": "https://github.com/<%= optGithubName%>/<%= optAppName %>",
	"authors": [
		"<%= optAuthorName %>"
	],
	"description": "<%= optAppDescription %>",
	"keywords": [
		<% for(var ii=0; ii<optAppKeywords.length; ii++) {
			if(ii !==0) {
				print('    ');
			}
			print('"'+optAppKeywords[ii]+'"');
			if(ii !==(optAppKeywords.length -1)) {
				print(',\n');
			}
		}
		%>
	],
	"dependencies": {
		"angular": "1.3.0-rc.3",
		"angular-animate": "1.3.0-rc.3",
		"angular-sanitize": "1.3.0-rc.3",
		"angular-touch": "1.3.0-rc.3",
		"angular-route": "1.3.0-rc.3",
		"angular-cookies": "1.3.0-rc.3",
		"bourbon": "latest",
		"font-awesome": "latest",
		"less-flexbox": "latest",
		"momentjs": "latest",
		"angular-string": "latest",
		"angular-array": "latest",
		"angular-google-auth": "latest",
		"angular-forminput": "latest",
		"angular-socket-io": "latest"
	},
	"license": "MIT",
	"resolutions": {
		"angular": "1.3.0-rc.3",
		"angular-animate": "1.3.0-rc.3",
		"angular-sanitize": "1.3.0-rc.3",
		"angular-touch": "1.3.0-rc.3",
		"angular-route": "1.3.0-rc.3",
		"angular-cookies": "1.3.0-rc.3"
	}
}
